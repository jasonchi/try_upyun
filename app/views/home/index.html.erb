<form action="http://v0.api.upyun.com/zhongyita/" method="post" enctype="Multipart/form-data" id="upload-form">
	<input type="hidden" name="policy" value="<%=@policy%>">
	<input type="hidden" name="signature" value="<%=@sig%>">
	<input type="file" name="file">
	<input type="file" name="file">
	<input type="submit">
</form>

<%=form_tag('http://v0.api.upyun.com/zhongyita/',:method=>true,:multipart=>true,:id=>"upload-form",:remote=>true) do %>
	<%=hidden_field_tag 'policy',@policy%>
	<%=hidden_field_tag 'signature',@sig%>
	<%=file_field_tag 'file'%>
	<%=submit_tag("提交")%>
<%end%>
